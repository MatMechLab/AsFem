{
	"description":{
		"domain":"the size must be [0,1]x[0,1]",
		"bcs":"phi(on the boundary)=1.0+x^2+2*y^2",
		"sigma":"sigma=1",
		"f":"f=-x*sin(y)",
		"equation":"div(sigma*(1+phi^2)*grad(phi))=f"
	},
	"mesh":{
		"type":"msh4",
		"file":"cubic.msh",
		"savemesh":true
	},
	"dofs":{
		"names":["phi"]
	},
	"elements":{
		"elmt1":{
			"type":"poisson",
			"dofs":["phi"],
			"material":{
				"type":"nonlinear-poisson3d",
				"parameters":{
					"sigma":1.0
				}
			}
		}
	},
	"projection":{
		"type":"default",
		"vectormate":["gradu"]
	},
	"bcs":{
		"fixed":{
			"type":"dirichlet",
			"dofs":["phi"],
			"bcvalue":1.0,
			"side":["back"]
		}
	},
	"linearsolver":{
		"type":"gmres",
		"preconditioner":"lu",
		"maxiters":10000,
		"tolerance":1.0e-16
	},
	"nlsolver":{
		"type":"asfem",
		"maxiters":50,
		"abs-tolerance":5.0e-7,
		"rel-tolerance":5.0e-10,
		"s-tolerance":0.0
	},
	"output":{
		"type":"vtu",
		"interval":1
	},
	"qpoints":{
		"bulk":{
			"type":"gauss-legendre",
			"order":2
		}
	},
	"job":{
		"type":"static",
		"print":"dep"
	}
}